{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/signup",
      "dest": "/api/signup.py"
    },
    {
      "src": "/api/login",
      "dest": "/api/login.py"
    },
    {
      "src": "/api/chat",
      "dest": "/api/chat.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/contact.html",
      "dest": "/static/contact.html"
    },
    {
      "src": "/terms.html",
      "dest": "/static/terms.html"
    },
    {
      "src": "/",
      "dest": "/static/index.html"
    }
  ]
}